{
  "name": "renec-youtube-sharing-database-migration",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
    "migrate:redo": "yarn migrate:rollback && yarn migrate:latest",
    "migrate:make": "knex --knexfile ./knexfile.ts migrate:make -x ts",
    "migrate:up": "knex --knexfile ./knexfile.ts migrate:up",
    "migrate:down": "knex --knexfile ./knexfile.ts migrate:down",
    "lint:fix": "eslint --fix --ext ts .",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "typescript": "^5.5.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn lint:fix"
    ]
  }
}
